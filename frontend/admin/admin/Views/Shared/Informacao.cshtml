@model IEnumerable<admin.ViewModels.CompanyPositionRequirementsVM>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="#">Home</a></li>
        <li class="breadcrumb-item"><a href="#">Library</a></li>
        <li class="breadcrumb-item active" aria-current="page">Data</li>
    </ol>

    <table class="table">
        <tr>
            <th>
                Id
            </th>
            <th>
                Career name
            </th>
            <th>
                Action
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.company_position_name )
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.company_position_id)
                </td>
                <td>
                    <button class="btn-details" @*onclick="location.href='@Url.Action("Details", new { id = item.career_map_id })'"*@>Exibir</button>
                </td>
            </tr>
        }
        <tr>
            <td>
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            </td>
        </tr>
    </table>
</nav>




